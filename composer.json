{
    "name": "cityhunter/laravel-meta-data",
    "description": "Simple meta data system for laravel",
    "type": "library",
    "keywords": [
        "laravel",
        "package"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Henoc Djabia",
            "email": "henoc35@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0|^8.1|^8.2|^8.3",
        "illuminate/support": "^8.0|^9.0|^10.0|^11",
        "illuminate/database": "^8.0|^9.0|^10.0|^11",
        "laravel/framework": "^8.0|^9.0|^10.0|^11"
    },
    "autoload": {
        "psr-4": {
            "CityHunter\\LaravelMetaData\\": "src/"
        },
        "files": [
            "./src/Helpers/user.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "CityHunter\\LaravelMetaData\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CityHunter\\LaravelMetaData\\LaravelMetaDataServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "^9.5.10",
        "phpro/grumphp": "^1.15",
        "squizlabs/php_codesniffer": "^3.7",
        "fakerphp/faker": "^1.21",
        "laravel/laravel": "^8.0|^9.0|^10.0",
        "mockery/mockery": "^1.5",
        "orchestra/testbench": "^8.0"
    },
    "config": {
        "allow-plugins": {
            "phpro/grumphp": true
        }
    }
}
